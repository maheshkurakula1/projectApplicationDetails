<!DOCTYPE html>
<html lang="en">
<head>
  <title>Bootstrap Example</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.js"></script>
  <style>
    /* Set height of the grid so .sidenav can be 100% (adjust if needed) */
    .row.content {height: 1500px}
    
    /* Set gray background color and 100% height */
    .sidenav {
      background-color: #f1f1f1;
      height: 100%;
    }
    
    /* Set black background color, white text and some padding */
    footer {
      background-color: #555;
      color: white;
      padding: 15px;
    }
    
    /* On small screens, set height to 'auto' for sidenav and grid */
    @media screen and (max-width: 100px) {
      .sidenav {
        height: auto;
        padding: 15px;
      }
      .row.content {height: auto;} 
    }
  </style>
</head>
<body>

<div class="container-fluid">
  <div class="row content">
    <div class="col-sm-2 sidenav">
      <h4>Dashboard</h4>
      <ul class="nav nav-pills nav-stacked">
        <li class="active"><a href="#section1">Madhapur</a></li>
        <li><a href="#section2">Film Nagar</a></li>
        <li><a href="#section3">Banglore</a></li>
        <li><a href="#section3">Jubliee Hills</a></li>
      </ul><br>
    </div>

    <div class="col-sm-9">
        <div class="card-body">
          <div class="row">
              <div class="col-md-6">
                  <canvas id="payment" height="200"></canvas>
              </div>
              <div class="col-md-6">
                <canvas id="myChart" height="200"></canvas>
              </div>
          </div>
      </div>
    </div>
  </div>
</div>

<footer class="container-fluid">
  <p>Footer Text</p>
</footer>

</body>
</html>
<script>
    $.ajax({
        url:'/getpaymentTypes',
        success: function(data){
            console.log(data.data.length)
            var ctx = document.getElementById('myChart');
            var myPieChart = new Chart(ctx, {
                    type: 'pie',
                    data: {
                        labels: ['Federal EDC','Yes Bank EDC','Dine-out'],
                        datasets:[
                            {
                                backgroundColor: ['#4554a3', '#ec9620', '#e52e63', '#e86190', '#8f3e96'],
                                data: [10,20,30]
                            }  
                        ]
                    },
                    options:{
                        responsive: true
                    }
              });
        }
    })
   
    var payment = document.getElementById('payment');
    var myPieChart = new Chart(payment, {
      type: 'line',
      data: {
        labels: ['Federal EDC','Yes Bank EDC','Dine-out','cash'],
        datasets:[
          {
            label:['iron hill madhapur'],
            data:[1000,250,8000,90]
          },
          {
            label:['iron hill film nagar'],
            data:[5000,1500,8000,900]
          }
        ]
      },
      options: {
          scales: {
              yAxes: [{
                  stacked: true
              }]
          }
      }
    });
</script>